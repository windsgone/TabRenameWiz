{
    "manifest_version": 3,
    "name": "__MSG_app_name__",
    "description": "__MSG_app_description__",
    "version": "1.0.0",
    "default_locale": "en",
    "permissions": ["tabs", "scripting", "activeTab"],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }